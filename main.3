#include <iostream>

int main() {
    int start, end, sum = 0;
    std::cout << "Enter the start of the range: ";
    std::cin >> start;
    std::cout << "Enter the end of the range: ";
    std::cin >> end;
    for (int i = start; i <= end; i++) {
        sum += i;
    }

    std::cout << "The sum of all numbers in the range [" << start << ", " << end << "] is " << sum << std::endl;
    return 0;
}
