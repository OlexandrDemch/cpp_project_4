#include <iostream>

using namespace std;

int main() {
    int a, b;
    cout << "Enter the first range limit: ";
    cin >> a;
    cout << "Enter the first range limit: ";
    cin >> b;
    if (a > b) {
        int temp = a;
        a = b;
        b = temp;
    }
    for (int i = a; i <= b; i++) {
        cout << i << " ";
    }
    cout << endl;

    return 0;
}
